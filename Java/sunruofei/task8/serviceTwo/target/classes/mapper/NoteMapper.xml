<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ptteng.dao.NoteMapper">

<insert id="insertCodePhone" parameterType="com.ptteng.model.Note">
insert into note (code,phone,create_at) VALUES (#{code},#{phone},#{createAt})
</insert>
    <select id="selectTime" parameterType="com.ptteng.model.Note" resultType="int" >
      select count(*) from note where #{dayStart} &lt;= create_at and create_at  &lt;= #{dayEnd} and phone = #{phone}
    </select>
</mapper>